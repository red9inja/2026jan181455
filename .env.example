# Frontend Environment Variables Template
# Copy this file to .env and update with your actual values

# AWS Configuration
REACT_APP_AWS_REGION=us-east-1

# Cognito Configuration
REACT_APP_USER_POOL_ID=us-east-1_xxxxxxxxx
REACT_APP_USER_POOL_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx
REACT_APP_IDENTITY_POOL_ID=us-east-1:xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx

# S3 Configuration
REACT_APP_S3_BUCKET=demo-storage-bucket

# API Endpoints
REACT_APP_API_ENDPOINT=https://your-alb-dns-name
REACT_APP_LAMBDA_API_ENDPOINT=https://your-api-gateway-url

# Environment
REACT_APP_ENVIRONMENT=development
